{% for variable_name, value in django_settings.items() %}
{{ variable_name }}="{{ value }}"
{% endfor %}
DJANGO_SECRET_KEY="{{ django_secret_key }}"
{% if sentry_dsn %}
RAVEN_DSN="{{ sentry_dsn }}"
SENTRY_ENVIRONMENT="{{ sentry_environment }}"
{% endif %}
DATABASE_URL="{{ database_url }}"
ALLOWED_HOST="{{ allowed_host }}"
ALLOWED_HOST_IP="{{ allowed_host_ip }}"
KONG_TRAFFIC_URL="{{ kong_traffic_url }}"
KONG_ADMIN_URL="{{ kong_admin_url }}"
REDIS_HOST="{{ django_redis_host }}"
HTTPLOG2_ENDPOINT="{{ kong_http_log2_endpoint }}"
URLS_PREFIX="{{ django_urls_prefix }}"
ANALYTICS_TID="{{ ga_tracking_id }}"
